<T3DataStructure>
	<sheets>
		<!--
			################################
			  SHEET General Settings
			################################
		-->
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>Cấu hình</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<!-- Application -->
					<settings.applications>
						<TCEforms>
							<label>Loại ắc quy</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>tx_nano_domain_model_application</allowed>
								<size>3</size>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<wizards>
									<suggest>
										<type>suggest</type>
										<default>
											<searchWholePhrase>1</searchWholePhrase>
										</default>
									</suggest>
								</wizards>
							</config>
						</TCEforms>
					</settings.applications>
					
					<!-- Limit Start -->
					<settings.limit>
						<TCEforms>
							<label>Số lượng hiển thị</label>
							<config>
								<type>input</type>
								<size>5</size>
								<eval>num</eval>
							</config>
						</TCEforms>
					</settings.limit>

				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>
